<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>

<SplitPane fx:id="rootPane" dividerPositions="0.4" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.swoeste.demo.gen.alg.ui.UIWorldController">
  <items>
      <ScrollPane fx:id="controlPane" prefHeight="200.0" prefWidth="200.0">
         <content>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="FPS:" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
                  <Label fx:id="fpsLabel" layoutX="41.0" layoutY="10.0" />
               </children>
            </AnchorPane>
         </content></ScrollPane>
      <ScrollPane fx:id="worldPane" prefHeight="200.0" prefWidth="200.0">
        <content>
            <Canvas fx:id="worldCanvas" height="500.0" width="500.0" />
        </content>
      </ScrollPane>
  </items>
</SplitPane>
