<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<TitledPane fx:id="pane" animated="false" prefWidth="300.0" text="Runtime Information" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.swoeste.demo.gen.alg.ui.controller.section.UIRuntimeInformationController">
    <content>
        <ScrollPane>
            <content>
                <GridPane prefWidth="298.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="FPS:" />
                        <Label fx:id="lblFPS" GridPane.columnIndex="1" />
                        <Label text="Age:" GridPane.rowIndex="1" />
                        <Label fx:id="lblAge" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label text="APS:" GridPane.rowIndex="2" />
                        <Label fx:id="lblAPS" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="Creatures (alive):" GridPane.rowIndex="3" />
                  <Label fx:id="lblCreatures" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label text="Creatures (dead):" GridPane.rowIndex="4" />
                  <Label fx:id="lblDeadCreatures" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </padding>
                </GridPane>
            </content>
        </ScrollPane>
    </content>
</TitledPane>
